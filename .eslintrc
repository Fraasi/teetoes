{
	"extends": "eslint-config-airbnb",
	"env":{"browser": true},
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, { "ignore": ["electron"] }],
    "linebreak-style": 0,
		"react/prefer-stateless-function": 0,
		"no-console": 0,
		"no-tabs": 0,
		"semi":0,
		"global-require": 0,
		"indent": 0,
		"class-methods-use-this": 0,
		"react/prop-types": 0

  }
}
